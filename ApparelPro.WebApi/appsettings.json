{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }, 
  "RefreshTokenExpiry": 1,  
  "PolicyManager": {
    "Merchandising": {
      "Roles": [ "Merchandiser,Order Entry Operator,Merchandiser Manager" ]
    },
    "Reports": {
      "Roles": [ "Inventory", "Production", "Import Export", "Merchandising", "OfficeAdmin" ]
    },
    "Stores": {
      "Roles": [ "Inventory" ]
    },
    "RegisteredUser": {
      "Roles": [ "" ]
    },
    "Admin": {
      "Roles": ["Administrator"]
    }
  },
  "AllowedHosts": "*", 
  "PageSize": 10
}
